{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Oh Hi Markdown",
	"extends": "./base.json",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"lib": ["DOM", "ES2019"],
		"module": "ESNext",
		"target": "es6",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"paths": {
			"~/*": ["../editor/src/"]
		}
	}
}
