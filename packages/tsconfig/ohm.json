{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Oh Hi Markdown",
	"extends": "./base.json",
	"compilerOptions": {
		"strict": false,
		"strictNullChecks": true,
		"noImplicitAny": false,
		"allowJs": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"lib": ["DOM", "ESNext"],
		"module": "ESNext",
		"target": "ESNext",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"paths": {
			"~/*": ["../oh-hi-markdown/src/"]
		}
	}
}
